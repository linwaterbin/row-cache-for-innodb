+		/* The buddy is in a free list */
+
+		if ((byte*)buddy < (byte*)area) {
+			new_ptr = ((byte*)buddy) + MEM_AREA_EXTRA_SIZE;
+
+			mem_area_set_size_out(buddy, 2 * size);
+			mem_area_set_free_out(buddy, FALSE);
+		} else {
+			new_ptr = ptr;
+
+			mem_area_set_size_out(area, 2 * size);
+		}
