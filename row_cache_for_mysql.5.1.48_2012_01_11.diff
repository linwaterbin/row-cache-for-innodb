+			ut_error;
+		}
+	}
+#endif
+	buddy = mem_area_get_buddy_out(area, size, pool);
+
+	n = ut_2_log(size);
+
+	mutex_enter(&(pool->mutex));
+
+
+	if (buddy && mem_area_get_free_out(buddy)
+	    && (size == mem_area_get_size_out(buddy))) {
